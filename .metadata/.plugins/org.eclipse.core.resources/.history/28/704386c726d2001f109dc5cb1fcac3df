package Day_06;

public class Classwork_p4 {

	public static void main(String[] args) {
		
		int [] marks = {70,80,90,60,50};
		int [] marks1= {67,77,55,76,60};
		
		int [] marks2= new int[marks.length];
		
		
		for(int i =0 ;i<marks.length;i++) {
			marks2[i] = marks[i] +marks1[i];
		}
		 for(int i =0;i<marks2.length;i++) {
			 System.out.println("index : " + i + marks2);
		 }

	}

}
