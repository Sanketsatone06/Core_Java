package Day_03;

public class Homework_p1 {
	public class Main {
	    public static void main(String[] args) {
	        int a = 55;
	        int b = 70;

	        if (a < 50 && a < b) {
	            System.out.println("Both conditions are true.");
	        } else {
	            System.out.println("Both conditions are not true.");
	        }
	    }
	}
