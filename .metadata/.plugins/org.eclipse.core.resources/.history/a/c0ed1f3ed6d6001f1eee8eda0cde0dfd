package practice;

import java.util.Scanner;

public class Ex_8 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number : ");
		
		int n = sc.nextInt();
		int sum = 0;
		int i = 1;
		while(i<=n) {
			sum =  sum+i;
			System.out.print(sum + i);
			i++;
		}
		
		
		

		
		
	}
}