package practice;

import java.util.Scanner;

public class Ex_6 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter a number");
		int num = sc.nextInt();
		
	  int fact=1;
	  
	  int i = 1;
	  
	  while( i <= num ) {
//		  fact=fact*num;
//		  System.out.println(fact*num);
		  i++;
	  }
		System.out.println(fact);

	}

}
