package practice;

import java.util.Scanner;

public class Ex_6 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("enter a number");
		int num = sc.nextInt();
		
		int fact = 1;
		
		int i=num; 
		while ( i > 1 ) {
			fact= fact * i;
			i--;
		}
		System.out.println(fact);
	}

}
