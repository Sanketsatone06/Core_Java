package Day_05;

public class Practice {

	public static void main(String[] args) {
		
		//1.Print 1 to 50 alternate numbers by using while loop
		
		int i = 1;
		while(i<=50) {
			System.out.println(i);
			i+=2;
		}
		
		//Print 1 to 50 even numbers by using while loop
		
		int a = 1;
		while(a<=50) {
			if(a%2==0) {
				System.out.println("is even" + a);
			}
			}
		}

	}


